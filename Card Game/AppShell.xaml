<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Card_Game.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Card_Game"
    Shell.FlyoutBehavior="Disabled"
    Title="Card_Game">

    <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />

    <ShellContent
                Title="Blackjack"
                ContentTemplate="{DataTemplate local:Blackjack}"
                Route="Blackjack" />

    <ShellContent
                Title="Settings_page"
                ContentTemplate="{DataTemplate local:Settings_page}"
                Route="Settings_page" />
</Shell>  
